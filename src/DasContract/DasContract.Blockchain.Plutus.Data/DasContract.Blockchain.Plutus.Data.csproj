<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <Version>1.0.0</Version>
    <Authors>Martin Drozd√≠k</Authors>
    <Description>Data models for PlutusContract and DasContract conversion</Description>
